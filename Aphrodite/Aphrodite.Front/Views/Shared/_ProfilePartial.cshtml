@Scripts.Render("~/Scripts/Facedate.js")
<div class="row">
    <div id="profile-box-wrapper" class="col-md-4 col-md-offset-4 ">
        @TempData["Match"]

        @if (Model != null)
        {
            <div id="profile-box" class="center-block">
                <div class="profile-image">
                    <img id="facedate_Image" src="@Url.Content(@ViewBag.PhotoFile)" class="photosize img-responsive" />
                </div>

                <div class="col-md-12 like_box">

                    <p>
                        @Model.DisplayName
                        @Model.Years jaar oud
                    </p>
                    <div class="col-md-12">
                        <div class="col-md-6">
                            @Html.ActionLink(" ", "Dislike", "Home", new { id = Model.ID }, new { id = "facedate_Dislike", onclick = "rate($(this).attr('href'));" })
                        </div>
                        <div class="col-md-6">
                            @Html.ActionLink(" ", "Like", "Home", new { id = Model.ID }, new { id = "facedate_Like", onclick = "rate($(this).attr('href'));" })
                        </div>
                    </div>
                </div>
            </div>
        }
        else
        {
            <div class="no-more-matches center-block">
                <label>Er zijn geen nieuwe profielen gevonden, probeer het later nog eens.</label>
            </div>
        }
    </div>
</div>